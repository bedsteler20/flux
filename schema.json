{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": false,
    "properties": {
        "variables": {
            "type": "array"
        },
        "theme": {
            "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/theme_data.json"
        },
        "properties": {
            "properties": {
                "useClientSideDecorations": {
                    "type": "boolean"
                }
            }
        },
        "breakpoints": {
            "properties": {
                "xs": {
                    "type": "number"
                },
                "sm": {
                    "type": "number"
                },
                "md": {
                    "type": "number"
                },
                "lg": {
                    "type": "number"
                },
                "xl": {
                    "type": "number"
                },
                "xxl": {
                    "type": "number"
                }
            }
        }
    }
}